{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run sys_expert",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/sys_expert",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build"
        }
    ]
}
